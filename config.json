{
  "event": {
    "attempts": 1,
    "timeout": 500
  },
  "log": {
    "level": "debug"
  },
  "redis": {
    "host": "localhost",
    "port": 6379,
    "prefix": "evd"
  },
  "schedule": [{
    "body": "{{this}}",
    "id": 1,
    "text": "every 1 minute"
  }],
  "server": {
    "count": 1,
    "port": 8080
  },
  "shared": {
    "tool": "eveningdriver",
    "format": "json"
  },
  "statsd": {
    "host": "localhost",
    "port": 8125
  },
  "transform": [{
    "class": "logging",
    "id": 1,
    "inputs": [],
    "opts": {}
  }],
  "worker": {
    "count": 2
  }
}
